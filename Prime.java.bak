class Prime 
{
	public static void main(String[] args) 
	{
		int a=5;
		int temp=0;
		for (int i=2;i<=a ;i++ )
		{
			if (a%i==0)
			{
				temp=temp+1;
			}
		}
		System.out.println(a);
		if (temp==0)
		{
			System.out.println("Number is prime number");
		}
		else{
			System.out.println("it is not prime number");
		}
	}
}
